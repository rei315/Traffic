# platform :ios, '9.0'

target 'Traffic' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'
  
  # Pods for Traffic

  target 'TrafficTests' do
    inherit! :search_paths
    pod 'Firebase'
    # Pods for testing
  end

  target 'TrafficUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
